{
  "name": "react-component-quick-start",
  "version": "1.0.0",
  "description": "A minimal React component",
  "main": "src/index.js",
  "module": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --config examples/webpack.config.js --host 0.0.0.0 --progress --profile",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production babel src -d dist",
    "prebuild-pages": "rimraf .tmp",
    "build-pages": "cross-env NODE_ENV=production webpack --config examples/webpack.config.js --progress --profile",
    "predeploy": "npm run build-pages",
    "deploy": "gh-pages -d .tmp",
    "lint": "eslint --ext .js,.jsx --fix .",
    "test": "cross-env NODE_ENV=test jest"
  },
  "author": "ofk",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "prop-types": "^15.6.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.3.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "collectCoverage": true
  }
}
